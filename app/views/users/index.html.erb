<div class="container">
  <div class="indexBox">
    <%= will_paginate %>
    <ul class="users">
      <h1>すべてのユーザー</h1>
      <% @users.each do |user| %>
        <li>
          <%= link_to user.name, user %>
          <% if current_user.admin? && !current_user?(user) %>
           |<%= link_to "delete", user, method: :delete,
                             data: { confirm: "You sure?" } %>
          <% end %>
        </li>
      <% end %>
    </ul>
    <%= will_paginate %>
  </div>
</div>
